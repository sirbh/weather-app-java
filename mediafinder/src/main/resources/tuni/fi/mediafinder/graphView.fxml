<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane fx:id="scrollPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tuni.fi.mediafinder.GraphViewController">
  <content>
    <AnchorPane fx:id="anchorPane">
         <children>
            <TabPane prefHeight="720.0" prefWidth="1280.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab closable="false" text="Amount per Rating">
                  <content>
                    <AnchorPane fx:id="ratingsTab">
                           <children>
                              <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToMainView" text="Back" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab text="Amount per Year">
                  <content>
                    <AnchorPane fx:id="yearsTab">
                           <children>
                              <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToMainView" text="Back" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </children></AnchorPane>
  </content>
</ScrollPane>
